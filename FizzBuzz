groot = range(1,101)
def divisible3 (input):
    for x in input:
        if x % 3 == 0:
            print "Fizz"
divisible3(groot)

def divisible5 (input):
    for x in input:
        if x % 5 == 0:
            print "Buzz"

divisible5(groot)

def divisibles (input):
    for x in input:
        if x % 3 == 0 and x % 5 == 0:
            print "FizzBuzz"
divisibles(groot)
